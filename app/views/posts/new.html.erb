<%= form_with model: @post, local: true do |f| %>
<%= render 'shared/error_messages', model: @post %>

<p><%= f.label :date %><br>
<%= f.date_field :date %> #titleの入力フォーム生成
</p>

<p><%= f.label :prefecture %><br>
<%= f.text_area :prefecture %> #bodyの入力フォーム生成
</p>
<p><%= f.label :municipality %><br>
<%= f.text_area :municipality %> #bodyの入力フォーム生成
</p>
<p><%= f.label :content %><br>
<%= f.text_field :content %> #bodyの入力フォーム生成
</p>
<p><%= f.label :number %><br>
<%= f.number_field :number %> #bodyの入力フォーム生成
</p>

<p><%= f.submit %> #送信ボタンを生成
</p>

<% end %>